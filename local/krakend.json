{
  "$schema": "https://www.krakend.io/schema/v2.10/krakend.json",
  "version": 3,
  "plugin": {
    "pattern": ".so",
    "folder": "/unleash/tentacles/"
  },
  "port": 10000,
  "extra_config": {
    "plugin/http-server": {
      "@comment_name": "Name order defines handler order. Last entry is outermost/first handler.",
      "name": [
        "agentcard-rw", "openai-a2a"
      ],
      "openai_a2a_config": {
        "agents": [
          {
            "model_id": "local/mock-agent",
            "url": "http://mock-agent:8000",
            "owned_by": "local",
            "createdAt": 1731679815
          }
        ]
      }
    },
    "router": {
      "disable_access_log": false,
      "hide_version_header": true
    }
  },
  "timeout": "360s",
  "output_encoding": "json",
  "name": "agent-gateway-krakend",
  "endpoints": [
    {
      "endpoint": "/local/mock-agent/.well-known/agent-card.json",
      "output_encoding": "no-op",
      "method": "GET",
      "backend": [
        {
          "host": [
            "http://mock-agent:8000"
          ],
          "url_pattern": "/.well-known/agent-card.json"
        }
      ]
    },
    {
      "endpoint": "/local/mock-agent",
      "output_encoding": "no-op",
      "method": "POST",
      "backend": [
        {
          "host": [
            "http://mock-agent:8000"
          ],
          "url_pattern": ""
        }
      ]
    }
  ]
}